<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/member_transactions"
    app:startDestination="@id/memberTransactionsFragment">
    <fragment
        android:id="@+id/memberTransactionsFragment"
        android:name="com.everydayapps.roomsix.ui.admin.fragments.MemberTransactionsFragment"
        android:label="Choose a member"
        tools:layout="@layout/fragment_member_transactions">
        <action
            android:id="@+id/action_memberTransactionsFragment_to_weeklyTransactionsFragment"
            app:destination="@+id/weeklyTransactionsFragment"/>
    </fragment>
    <fragment
        android:id="@+id/weeklyTransactionsFragment"
        android:name="com.everydayapps.roomsix.ui.admin.fragments.MemberWeeklyFragment"
        android:label="Weekly Transactions"
        tools:layout="@layout/fragment_member_weekly">
        <action
            android:id="@+id/action_weeklyTransactionsFragment_to_transactionFragment"
            app:destination="@+id/transactionFragment"/>
        <action
            android:id="@+id/action_weeklyTransactionsFragment_to_memberTransactionsFragment"
            app:destination="@+id/memberTransactionsFragment"/>
    </fragment>
    <fragment
        android:id="@+id/transactionFragment"
        android:name="com.everydayapps.roomsix.ui.admin.fragments.TransactionFragment"
        android:label="Transactions"
        tools:layout="@layout/fragment_transaction">
        <action
            android:id="@+id/action_transactionFragment_to_weeklyTransactionsFragment"
            app:destination="@+id/weeklyTransactionsFragment"/>
    </fragment>
</navigation>